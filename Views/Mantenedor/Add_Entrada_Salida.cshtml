<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Login y Registro</title>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet">


    <link href="~/assets/css/entrada_salida.css" rel="stylesheet" />

    <link href="~/plugins/noty/lib/noty.css" rel="stylesheet" />
    <link href="~/plugins/noty/lib/themes/sunset.css" rel="stylesheet" />
</head>
<body>
    <main>
        <div class="contenedor__todo">
            <div class="caja__trasera">
                <div class="caja__trasera-login">
                    @*<h3>Ya tienes una cue</h3>
                    <p>Inicia sesion para entrar a la pagina</p>
                    <button id="btn__iniciar-sesion">Iniciar Sesion</button>*@
                </div>

                <div class="caja__trasera-register" height: 200px;>
                    <h3>QR </h3>
                    <div id="carouselExampleIndicators" class="carousel slide slider-medicien " data-ride="carousel">
                        <ol class="carousel-indicators pelotitas" id="li">
                            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
             
                        </ol>
                      
                        <div class="carousel-inner" id="badcarrusel">
                            <div class="carousel-item active">
                                <p>Tiene que presionar el boton Generar QR</p>
                            </div>
                            <div id="borrar">

                            </div>

                        </div>

                        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                </div>
            </div>

            <div class="contenedor__login-register">
                <form action="/" class="formulario__login">
                    <h2>Entrada/Salida</h2>
                    <div class="text-center">
                        <div class="overlay">
                            <h2 class="label radius-circle" id="clock1"></h2>
                        </div>
                    </div>

                    <div class="text-center">
                        <button id="Add_Entrasda_Salida">Generar QR</button>
                  
                    </div>

                </form>
                <form action="/" class="formulario__register">
                    <h2>Entrada/Salida</h2>
                    <div class="text-center">
                        <div class="overlay">
                            <h2 class="label radius-circle" id="clock1"></h2>
                        </div>
                    </div>

                    <div class="text-center">
                        <button id="Add_Entrasda_Salida">Entrar</button>
                        <button id="Add_Salida">Salida</button>
                    </div>>
                </form>

            </div>
        </div>
    </main>
    
    <script src="~/assets/css/bootstrap-4.3.1-dist/js/bootstrap.min.js"></script>
    <script src="~/plugins/noty/lib/bounce.min.js"></script>
    <script src="~/plugins/noty/lib/noty.js"></script>
    <script src="~/assets/LoadPage/LoadPage.js"></script>
    <script src="~/assets/js/Mantendores/QRentrada_salida.js"></script>

</body>
</html>

